<!--pages/idcard/idcard.wxml-->
<import src="../../template/na_tabBar_template.wxml" />
<template is="na_tabBar_template" data="{{na_tabbar}}" />
<view class="page">

  <view class="page__bd idcard_bd">
    <view class="idcard_tip">上传本人身份证照片</view>

    <image id="imgUp" class="idcard_img" src="{{worksImgs[0]}}" bindtap="imgClickTab"></image>
    <image id="imgDown" bindtap="imgClickTab" class="idcard_img idcard_img_down" src="{{worksImgs[1]}}"></image>
    <button class="submit_btn" bindtap="submitClick" disabled="{{butDisabled}}">提交</button>

  </view>


 
</view>